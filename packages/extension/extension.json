{
  "id": "builtin.markdown-preview",
  "name": "Media Preview",
  "description": "Markdown Preview",
  "browser": "src/markdownPreviewMain.ts",
  "activation": ["onWebView:builtin.markdown-preview"],
  "languages": [],
  "webViews": [
    {
      "id": "builtin.markdown-preview",
      "rpc": true,
      "path": "media/index.html",
      "selector": [".md"],
      "elements": [
        {
          "type": "title",
          "value": "Markdown Preview"
        },
        {
          "type": "script",
          "path": "media/index.js"
        },
        {
          "type": "css",
          "path": "media/index.css"
        }
      ]
    }
  ]
}
